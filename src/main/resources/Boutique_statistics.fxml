<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.chart.*?>
<?import controllers.Boutique_statisticsController?>

<VBox fx:id="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Boutique_statisticsController">
    <children>
        <BarChart fx:id="barChart" layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
            <xAxis>
                <CategoryAxis fx:id="xAxis" side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" side="LEFT" lowerBound="1" upperBound="10"/>
            </yAxis>
        </BarChart>
    </children>
</VBox>
